{
  "name": "ethan-post-portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "node esbuild.config.mjs",
    "dev:build": "node esbuild.config.mjs --watch",
    "dev:serve": "live-server dist --port=5173 --watch=dist",
    "dev": "npm-run-all --parallel dev:build dev:serve"
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5"
  }
}
